{
  "name": "data-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "stage:deploy": "wrangler deploy --env stage",
    "production:deploy": "wrangler deploy --env production",
    "dev": "wrangler dev --env stage --persist-to ../../.wrangler/state",
    "start": "wrangler dev --persist-to ../../.wrangler/state",
    "test": "vitest",
    "test:server": "vitest --project server",
    "stage:cf-typegen": "wrangler types --env-interface BaseEnv --env stage",
    "cf-typegen": "wrangler types --env-interface BaseEnv --env stage",
    "cf-migrate": "wrangler d1 migrations apply --env stage --local smart-links-stage --persist-to ../../.wrangler/state",
    "lint:fix": "biome check . --write",
    "lint": "biome check ."
  },
  "devDependencies": {
    "@cloudflare/puppeteer": "^1.0.4",
    "@cloudflare/vitest-pool-workers": "^0.10.2",
    "@types/node": "^24.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "webforai": "^2.1.1",
    "wrangler": "^4.45.2"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "@repo/data-ops": "workspace:*",
    "ai": "^5.0.82",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.3",
    "workers-ai-provider": "^2.0.0",
    "zod": "^4.1.12"
  }
}
